include ../dropdown-filter/selection-dates/selection-dates.pug
mixin dddate(title1,title2)
    .dropdown_date
        .dropdown_date-from
            h3= title1
            +selDate("dpickerfrom")(class='date-from')
        .dropdown_date-to
            h3= title2
            +selDate("dpickerto")(class='date-to')

+dddate("date dropdown","date dropdown")